<?xml version="1.0"?>

<PropertyList>

	<path>../../../Models/FlightDeck/FrontPanel/pfd.ac</path> 
	<texture-path>../../../Textures/Instruments</texture-path>

<animation>
   <type>select</type>
   <object-name>FDbars</object-name>
   <condition>
   <and>
     <or>
       <equals>
         <property>sim/current-view/view-number</property>
         <value>0</value>
       </equals>
       <equals>
         <property>sim/current-view/view-number</property>
         <value>9</value>
       </equals>
     </or>
     <equals>
       <property>/instrumentation/flightdirector/fd-on</property>
       <value>1</value>
     </equals>
   </and>
   </condition>
 </animation>
 
  <animation>
   <type>select</type>
   <object-name>GSscale</object-name>
   <condition>
     <and>
       <or>
         <equals>
           <property>sim/current-view/view-number</property>
           <value>0</value>
         </equals>
	 <equals>
           <property>sim/current-view/view-number</property>
           <value>9</value>
         </equals>
       </or>
       <property>/instrumentation/nav/has-gs</property>
       <property>/instrumentation/nav/in-range</property>
     </and>
   </condition>
 </animation>

 <animation>
   <type>select</type>
   <object-name>GSneedle</object-name>
   <condition>
     <and>
       <or>
         <equals>
           <property>sim/current-view/view-number</property>
           <value>0</value>
         </equals>
         <equals>
           <property>sim/current-view/view-number</property>
           <value>9</value>
         </equals>
       </or>
       <!-- property>/instrumentation/nav/has-gs</property -->
       <property>/instrumentation/nav/gs-in-range</property>
     </and>
   </condition>
 </animation>

 <animation>
   <type>translate</type>
   <object-name>GSneedle</object-name>
   <property>/instrumentation/nav/gs-needle-deflection</property>
   <interpolation>
     <entry>
       <ind> -10.0</ind>
       <dep>-0.05</dep>
     </entry>
     <entry>
       <ind> 0.0</ind>
       <dep>0.0</dep>
     </entry>
     <entry>
       <ind> 10.0</ind>
       <dep>0.05</dep>
     </entry>
  </interpolation>
  <!-- interpolation>
    <entry>
      <ind></ind>
      <dep></dep>
    </entry>
    <entry>
      <ind>0.0</ind>
      <dep>0.0</dep>
    </entry>
    <entry>
      <ind></ind>
      <dep></dep>
    </entry>
  </interpolation -->
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>
 
