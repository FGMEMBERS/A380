 <animation>
   <type>rotate</type>
   <object-name>VSIneedle</object-name>
   <!-- property>/autopilot/internal/vert-speed-fpm</property -->
   <property>/instrumentation/vertical-speed-indicator/indicated-speed-fpm</property>
   <interpolation>
     <entry>
       <ind>-6000.0</ind>
       <dep>-50.0</dep>
     </entry>
     <entry>
       <ind>-1000.0</ind>
       <dep>-25.0</dep>
     </entry>
     <entry>
       <ind>0.0</ind>
       <dep>0.0</dep>
     </entry>
     <entry>
       <ind>1000.0</ind>
       <dep>25.0</dep>
     </entry>
     <entry>
       <ind>6000.0</ind>
       <dep>50.0</dep>
     </entry>
   </interpolation>
   <axis>
     <x>-1.0</x>
     <y>0.0</y>
     <z>0.0</z>
   </axis>
   <center>
     <x-m>0.00</x-m>
     <y-m>0.084</y-m>
     <z-m>0.004</z-m>
   </center>
 </animation>

