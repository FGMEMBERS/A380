<?xml version="1.0"?>
<PropertyList>

<name>MFD Panel</name>
<background>Aircraft/A380/Models/Instruments/MFD/black.png</background>
<w>256</w>
<h>256</h>

<instruments> 

    <instrument include="ROSE.xml">
        <condition>
            <and>
                <greater-than>
                    <property>systems/electrical/outputs/efis</property>
                    <value>15</value>
                </greater-than>
                <equals>
                    <property>instrumentation/efis[0]/mfd/display-mode</property>
                    <value>ROSE</value>
                </equals>
            </and>
        </condition>
        <name>rose</name>
        <x>128</x>
        <y>128</y>
        <w>256</w>
        <h>256</h>
    </instrument>
    
